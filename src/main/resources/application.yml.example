spring:
  datasource:
    url: jdbc:mysql://localhost:3306/forumcalendar?useSSL=false
    username: root
    password:
    initialization-mode: always
    platform: all

  jpa:
    hibernate:
      ddl-auto: create-drop

    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL5InnoDBDialect

logging:
  level:
    org:
      hibernate:
        SQL: DEBUG
        type: TRACE